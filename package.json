    "dev": "tsup --watch",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@hcl-cdp-ta/hclcdp-web-sdk": "^1.1.2",
    "react": "^19.0.0",
    "ua-parser-js": "^2.0.0",
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/node": "^24.0.0",
    "@types/react": "^19.0.8",
    "husky": "^9.1.6",
    "terser": "^5.39.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  }
