{
  "name": "@hcl-cdp-ta/hclcdp-web-sdk-react",
  "version": "1.1.2",
  "description": "HCL CDP Web SDK React Wrapper",
  "keywords": [
    "HCLCDP"
  ],
  "homepage": "https://github.com/HCL-CDP-TA/hclcdp-web-sdk-react#readme",
  "bugs": {
    "url": "https://github.com/HCL-CDP-TA/hclcdp-web-sdk-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HCL-CDP-TA/hclcdp-web-sdk-react.git"
  },
  "license": "MIT",
  "author": "Simon Pallister, HCL Software",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "main": "./dist/hclcdp-react.cjs",
  "module": "./dist/hclcdp-react.js",
